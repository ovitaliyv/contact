<script>
  $('.dropdown-toggle').dropdown();
  var main_menu=new main_menu.dd("main_menu");    
  main_menu.init("main_menu","menuhover");  
  $('a[href]').each(function(){
    var isHomepage = ($(this).attr('href')=="{{ path('VetalSiteBundle_homepageSlash') }}")?true:false;
    if ((isHomepage)&&($(this).attr('href')==location.pathname)){
      $(this).addClass('active');
    }else{
      var cont = location.pathname.search($(this).attr('href'));
      if((cont>-1)&&(!isHomepage)){$(this).addClass('active')}
    }
  });
</script>

